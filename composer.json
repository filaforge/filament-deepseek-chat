{
  "name": "filament/deepseek-chat",
  "description": "Filament panel plugin providing a DeepSeek chat page with admin-manageable API key.",
  "type": "library",
  "license": "MIT",
  "version": "0.1.0",
  "keywords": [
    "Laravel",
    "Filament",
    "Platform"
  ],
  "homepage": "https://github.com/filaforge/deepseek-chat",
  "authors": [
    {
      "name": "Filaforge"
    }
  ],
  "support": {
    "email": "filaforger@gmail.com",
    "source": "https://github.com/filaforge/deepseek-chat"
  },
  "require": {
    "php": "^8.1",
    "filament/filament": "^4.0",
    "illuminate/support": "^12.0",
    "spatie/laravel-package-tools": "^1.16",
    "guzzlehttp/guzzle": "^7.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Filaforge\\DeepseekChat\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Filaforge\\DeepseekChat\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Filaforge\\DeepseekChat\\Providers\\DeepseekChatServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
